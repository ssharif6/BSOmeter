// This is the client class that will gather all the urls from different search engines and websites
import java.util.*;
public class NewsCompiler {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		BingAPI bingApi = new BingAPI("Donald Trump Republican Debate");
		bingApi.queryForUrls();
		HashSet<String> bingUrlSet = bingApi.getListofUrls();
	}

}
